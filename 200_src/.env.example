# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_NETWORK=devnet
SOLANA_COMMITMENT=confirmed

# Logging
LOG_LEVEL=info
NODE_ENV=development

# Performance
BATCH_SIZE=100
CONCURRENT_REQUESTS=10

# Cache
CACHE_TTL=300000
CACHE_MAX_SIZE=1000

# Security
ENCRYPT_KEYS=true
SESSION_TIMEOUT=3600000