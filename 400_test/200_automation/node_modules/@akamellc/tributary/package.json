{
  "name": "@akamellc/tributary",
  "version": "0.2.4",
  "description": "Solana token distribution system",
  "main": "dist/index.js",
  "bin": {
    "tributary": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli.js",
    "prepare": "husky",
    "prepublishOnly": "npm run typecheck && npm run lint && npm test && npm run build",
    "version:patch": "npm version patch && git push origin main --tags",
    "version:minor": "npm version minor && git push origin main --tags",
    "version:major": "npm version major && git push origin main --tags",
    "release": "npm run version:patch && npm publish",
    "release:minor": "npm run version:minor && npm publish",
    "release:major": "npm run version:major && npm publish"
  },
  "keywords": [
    "solana",
    "token",
    "distribution",
    "cli",
    "blockchain",
    "spl-token",
    "cryptocurrency"
  ],
  "author": "akameGusya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akameGusya/tributary.git"
  },
  "homepage": "https://github.com/akameGusya/tributary#readme",
  "bugs": {
    "url": "https://github.com/akameGusya/tributary/issues"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "inquirer": "^8.2.6",
    "toml": "^3.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "tributary-parameters.example.json"
  ],
  "publishConfig": {
    "access": "public"
  }
}
